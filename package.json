{
  "name": "ceropath-uploader",
  "version": "0.0.0",
  "description": "An OSX tool for uploading data in Ceropath",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "package": "ember build --env=production && electron-packager . CeropathUploader --platform=darwin --arch=x64 --version=0.34.3 --icon=icon.icns",
    "package-dev": "ember build && electron-packager . CeropathUploaderDev --platform=darwin --arch=x64 --version=0.34.3 --icon=icon.icns",
    "start": "ember server",
    "test": "ember test"
  },
  "repository": "",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.1.2",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "0.34.3",
    "ember-bootstrap-hurry": "0.0.1",
    "ember-cli": "1.13.8",
    "ember-cli-app-version": "0.5.0",
    "ember-cli-babel": "^5.1.3",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "^1.0.1",
    "ember-cli-htmlbars": "0.7.9",
    "ember-cli-htmlbars-inline-precompile": "^0.2.0",
    "ember-cli-ic-ajax": "0.2.1",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-qunit": "^1.0.0",
    "ember-cli-release": "0.2.3",
    "ember-cli-sri": "^1.0.3",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "1.13.8",
    "ember-disable-proxy-controllers": "^1.0.0",
    "ember-electron": "0.1.7",
    "ember-export-application-global": "^1.0.3",
    "ember-offline": "0.1.0"
  },
  "main": "electron.js"
}
